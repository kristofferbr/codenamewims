<?xml version="1.0" encoding="utf-8"?>
<sw805f16.codenamewims.RelativeItemLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativeItemLayout"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/list_item_height">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/label"
        android:text="@+id/label"
        android:textSize="@dimen/list_item_height"
        android:layout_alignParentLeft="true"/>

    <ImageView
        android:id="@+id/icon"
        android:layout_width="@dimen/list_item_height"
        android:layout_height="@dimen/list_item_height"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"/>

</sw805f16.codenamewims.RelativeItemLayout>